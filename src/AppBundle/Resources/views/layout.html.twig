<html lang="ru" ng-app="app">
<head>
    <meta charset="utf-8">
    <title>Mobium Panel</title>
    <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>
    {% stylesheets
    "@AppBundle_old/Resources/public/vendor/bootstrap/dist/css/bootstrap.min.css"
    "@AppBundle_old/Resources/public/css/*.min.css"
    "@AppBundle_old/Resources/public/css/*/*.min.css"
    "@AppBundle_old/Resources/public/vendor/*/*/*.min.css" output="css/style.css"
    %}
    <link rel="stylesheet" href="{{ asset_url }}" type="text/css"/>
    {% endstylesheets %}
    <link href="//cdnjs.cloudflare.com/ajax/libs/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <link href="//code.ionicframework.com/ionicons/1.5.2/css/ionicons.min.css" rel="stylesheet" type="text/css" />
    <link rel='stylesheet' href='//cdnjs.cloudflare.com/ajax/libs/angular-loading-bar/0.7.1/loading-bar.min.css' type='text/css' media='all' />
    <style>[ng-cloak] { display: none; }</style>
</head>
<body class="skin-blue">
    <div ui-view></div>

    {% javascripts
    "@AppBundle_old/Resources/public/vendor/cryptojslib/rollups/sha1.js"
    "@AppBundle_old/Resources/public/vendor/cryptojslib/rollups/sha512.js"
    "@AppBundle_old/Resources/public/vendor/cryptojslib/rollups/md5.js"
    "@AppBundle_old/Resources/public/vendor/underscore/underscore-min.js"
    "@AppBundle_old/Resources/public/vendor/jquery/dist/jquery.min.js"
    "@AppBundle_old/Resources/public/vendor/angular/angular.min.js"
    "@AppBundle_old/Resources/public/vendor/bootstrap/dist/js/bootstrap.min.js"
    "@AppBundle_old/Resources/public/vendor/angular-resource/angular-resource.min.js"
    "@AppBundle_old/Resources/public/vendor/angular-translate/angular-translate.min.js"
    "@AppBundle_old/Resources/public/vendor/angular-translate-loader-static-files/angular-translate-loader-static-files.min.js"
    "@AppBundle_old/Resources/public/vendor/angular-cookies/angular-cookies.min.js"
    "@AppBundle_old/Resources/public/vendor/angular-sanitize/angular-sanitize.min.js"
    "@AppBundle_old/Resources/public/vendor/angular-animate/angular-animate.min.js"
    "@AppBundle_old/Resources/public/vendor/angular-bootstrap/ui-bootstrap-tpls.min.js"
    "@AppBundle_old/Resources/public/vendor/angular-ui-router/release/angular-ui-router.min.js"
    "@AppBundle_old/Resources/public/vendor/angular-loading-bar/build/loading-bar.min.js"
    "@AppBundle_old/Resources/public/vendor/angular-ui-scroll/dist/ui-scroll.min.js"
    "@AppBundle_old/Resources/public/vendor/angular-ui-scrollpoint/dist/scrollpoint.min.js"
    "@AppBundle_old/Resources/public/vendor/angular-ui-event/dist/event.min.js"
    "@AppBundle_old/Resources/public/vendor/angular-ui-mask/dist/mask.min.js"
    "@AppBundle_old/Resources/public/vendor/angular-ui-validate/dist/validate.min.js"
    "@AppBundle_old/Resources/public/vendor/angular-ui-indeterminate/dist/indeterminate.min.js"
    "@AppBundle_old/Resources/public/vendor/angular-ui-uploader/dist/uploader.min.js"
    "@AppBundle_old/Resources/public/vendor/angular-ui-utils/index.js"
    "@AppBundle_old/Resources/public/vendor/angular-ui-tree/dist/angular-ui-tree.min.js"
    "@AppBundle_old/Resources/public/vendor/angular-bootstrap-nav-tree/dist/abn_tree_directive.js"
    "@AppBundle_old/Resources/public/scripts/app.js"
    "@AppBundle_old/Resources/public/scripts/*.js"
    "@AppBundle_old/Resources/public/scripts/deployd/*.js"
    "@AppBundle_old/Resources/public/scripts/services/*.js"
    "@AppBundle_old/Resources/public/scripts/directives/*.js"
    "@AppBundle_old/Resources/public/scripts/modules/*.js"
    "@AppBundle_old/Resources/public/scripts/controllers/*.js"
    %}
    <script src="{{ asset_url  }}"></script>
    {% endjavascripts %}
</body>
</html>